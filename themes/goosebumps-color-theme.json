    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "string.comment"
            ],
            "settings": {
                "foreground": "#00FF00"
            }
        },
        {
            "scope": [
                "constant",
                "entity.name.constant",
                "variable.other.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#4E9A06"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#00FF00"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#4E9A06"
            }
        },
        {
            "scope": [
                "storage",
                "storage.type"
            ],
            "settings": {
                "foreground": "#4E9A06"
            }
        },
        {
            "scope": [
                "entity.name.class",
                "entity.name.type.class"
            ],
            "settings": {
                "foreground": "#4E9A06"
            }
        },
        {
            "scope": "meta.class",
            "settings": {
                "foreground": "#00FF00"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#CE9178"
            }
        },
        {
            "scope": "support.function",
            "settings": {
                "foreground": "#DCDCAA"
            }
        },
        {
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#DCDCAA"
            }
        },
        {
            "scope": "variable.parameter",
            "settings": {
                "foreground": "#00FF00"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#4E9A06"
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#F44747"
            }
        },
        {
            "scope": "source.js",
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
            "scope": "string.quoted.double.js",
            "settings": {
                "foreground": "#CE9178"
            }
        },
        {
            "scope": "support.function.console.js",
            "settings": {
                "foreground": "#DCDCAA"
            }
        },
        {
            "scope": "variable.other.readwrite.js",
            "settings": {
                "foreground": "#00FF00"
            }
        },
        {
            "scope": "punctuation.definition.string.begin.js",
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
            "scope": "punctuation.definition.string.end.js",
            "settings": {
                "foreground": "#FFFFFF"
            }
        }
